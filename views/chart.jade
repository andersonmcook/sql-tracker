extends header

block #content
  .chart
    a(href= msg.url target='_blank' title='' style='display: block; text-align: center;')
      img(src=`${msg.url}.png` alt='' style='max-width: 75%; width: 600px;' width='600' onerror='this.onerror=null;this.src="https://plot.ly/404.png";')
    script(data-plotly=`threepears:${msg.file}` src='https://plot.ly/embed.js' async)
